#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[]){
    int n1;
    int n2;
    puts("ingrese dos numeros: ");
    scanf("%d", &n1);
    scanf("%d", &n2);
    while(n2 > n1){
        if(n2 % 2 == 0){
            printf("%d ", n2);
        }
        n2--;
    }
    printf("\n");
    return 0;

}
