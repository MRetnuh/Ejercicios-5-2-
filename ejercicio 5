#include <stdio.h>

int main(int argc, char *argv[]) {
    int nota;
    puts("Ingrese su nota: ");
    scanf("%d", &nota);
    if(nota > 6){
        puts("Aprobado");
    }
    if(nota >= 8){
        puts("Felicitaciones");
    }
    else{
        puts("Debes esforzarte mas");
    }
    return 0;
}
